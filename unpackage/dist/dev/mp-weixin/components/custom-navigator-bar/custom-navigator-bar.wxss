20% {
    --mycolor-a: #f79188;
    --mycolor-b: red;
    --mycolor-c: fuchsia;
}
40% {
    --mycolor-a: #ff3c41;
    --mycolor-b: #e228a0;
    --mycolor-c: #2e4c96;
}
