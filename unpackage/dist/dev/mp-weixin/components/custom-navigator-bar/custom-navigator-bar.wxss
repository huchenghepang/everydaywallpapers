@property --mycolor-a {
  syntax: "<color>";
  inherits: false;
  initial-value: fuchsia;
}
@property --mycolor-b {
  syntax: "<color>";
  inherits: false;
  initial-value: #f79188;
}
@property --mycolor-c {
  syntax: "<color>";
  inherits: false;
  initial-value: red;
}
